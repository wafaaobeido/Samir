@model Models.Product

<div class="col-md-4">
    <img Class="Product_Image" src="@Url.Content(Model.Picture[0])" alt="Foto van dit object" />

    <div class="info">
        <h5 class="name">
           @Html.ActionLink(Model.Name, "ViewProductDetails", new { id = Model.Id })
        </h5>
        <h5 class="price">
            € @Model.Price
        </h5>
        
    </div>
</div>


